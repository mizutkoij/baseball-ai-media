exports[`HTTP Recording/Replay > game detail fetching > fetches game preview page > game-detail-html 1`] = `
"<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <title>NPB Game Preview</title>
</head>
<body>
    <!-- NPB先発投手詳細サンプルHTML -->
    <div class="preview-container">
        <div class="starting-pitchers">
            <div class="home-pitcher">
                <div class="pitcher-name">菅野 智之</div>
                <div class="pitcher-hand">右</div>
                <div class="pitcher-stats">
                    <span class="era">2.45</span>
                    <span class="wins">12</span>
                    <span class="losses">4</span>
                </div>
            </div>
            
            <div class="away-pitcher">
                <div class="pitcher-name">髙橋 遥人</div>
                <div class="pitcher-hand">左</div>
                <div class="pitcher-stats">
                    <span class="era">3.12</span>
                    <span class="wins">8</span>
                    <span class="losses">6</span>
                </div>
            </div>
        </div>
        
        <!-- 代替セレクタパターン -->
        <div class="starting-lineups">
            <div class="home-team-starter">
                <div class="name">山本 由伸</div>
                <div class="throw">右投</div>
                <div class="record">15勝3敗 防御率1.98</div>
            </div>
            
            <div class="away-team-starter">
                <div class="name">有原 航平</div>
                <div class="throw">右投</div>
                <div class="record">9勝7敗 防御率2.87</div>
            </div>
        </div>
    </div>
</body>
</html>"
`;

